            <script type="text/javascript">
                var nextIndex = 0;
                var	prevIndex = 0;
                var nextIndex2 = 0;
                var	prevIndex2 = 0;
                var index = 0;

                function home_onAfter(curr, next, opts) {
                    index = opts.currSlide;
                    /*$('.prev')[index == 0 ? 'hide' : 'show']();
                    $('.next')[index == opts.slideCount - 1 ? 'hide' : 'show']();
                    $(next).addClass('active');*/

                    nextIndex = index + 1;
                    prevIndex = index - 1;

                    $('#pagination_ul li a').each(function(index) {
                        $(this).removeClass('act_2')
                    });
                    index = index+1;
                    $('#pagination_ul li a#'+index).addClass('act_2');
                }

                function landing_onAfter(curr, next, opts) {
                    index = opts.currSlide;
                    /*$('.prev')[index == 0 ? 'hide' : 'show']();
                    $('.next')[index == opts.slideCount - 1 ? 'hide' : 'show']();
                    $(next).addClass('active');*/

                    nextIndex2 = index + 1;
                    prevIndex2 = index - 1;

                    $('#pagination_ul2 li a').each(function(index) {
                        $(this).removeClass('act_3')
                    });
                    index = index+1;
                    $('#pagination_ul2 li a#'+index).addClass('act_3');
                }
                $( window ).load(function() {
                    $('#home_slider').cycle({
                        fx:     'scrollLeft',
                        slideResize: true,
                        containerResize: true,
                        width: '100%',
                        height: 'auto',
                        after: home_onAfter,
                        fit: 1,
                        timeout: 5000,
                        speed:   1000
                    });

                    $('#landing_slider').cycle({
                        fx:     'scrollLeft',
                        slideResize: true,
                        containerResize: true,
                        width: '100%',
                        height: 'auto',
                        after: landing_onAfter,
                        fit: 1,
                        timeout: 5000,
                        speed:   1000
                    });
                });
                $(function(){
                    $(".next_arw").click(function () {
                        $('#landing_slider').cycle(nextIndex2, "scrollLeft");
                    });

                    $(".prev_arw").click(function () {
                        $('#landing_slider').cycle(prevIndex2, "scrollRight");
                    });

                    $("#pagination_ul li a").click(function () {
                        var id = parseInt($(this).attr('id'));
                        //alert(index);
                        if(id>index-1){
                        //alert(id);
                            $('#home_slider').cycle(id-1, "scrollLeft");
                        }else{
                        //alert(id);
                            $('#home_slider').cycle(id-1, "scrollRight");
                        }
                    });

                    $("#pagination_ul2 li a").click(function () {
                        var id = parseInt($(this).attr('id'));
                        if(id>index-1){
                            $('#landing_slider').cycle(id-1, "scrollLeft");
                        }else{
                            $('#landing_slider').cycle(id-1, "scrollRight");
                        }
                    });

                    $("#landing_slider li").each(function () {
                        var height1 = 0;
                        var height = $(this).find('img').height();
                        if(height>height1){
                            height1 = height;
                        }
                        var div_height = $(this).find('.content_box').height();
                        //alert(div_height);
                    });

                    $("#toTop").click(function() {
                        $("html, body").animate({scrollTop: $('#hashblock').offset().top }, 1000);
                    });

                });
            </script>
